<h1>Edit User<h1>

<%= form_for @user do |f| %>
   <p>
      <%= f.label :name%>
      <%= f.text_field :name%>
   </p>
   <p>
      <%= f.label :password%>
      <%= f.text_field :password%>
   </p>
   <p>
      <%= f.label :profile%>
      <%= f.text_area :profile%>
   </p>
   <p><%= f.submit "Submit"%></p>

<% end %>
